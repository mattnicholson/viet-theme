{% macro render(props) %}
<div x-data="keenslider" class="carousel keen-slider--off absolute top-0 left-0 w-full h-full">
    {% for i in props.images %}
      <div class="fader__slide keen-slider__slide"><div class="Image" x-data="image" x-bind="lazy" data-fit="cover" data-src="{{ i }}"></div></div>
    {%  endfor %}
</div>
